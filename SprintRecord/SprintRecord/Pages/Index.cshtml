@page
@model IndexModel
@{
    ViewData["Title"] = "Home page";
}



<div class="card-columns">
    @foreach (var team in Model.Teams)
    {
        <div class="card">
            <div class="card-body">
                <div class="card-title">@team.Name</div>
                @foreach (var sprint in team.Sprints)
                {
                    <div class="card-text">@sprint.TeamCapacity</div>
                }
            </div>
        </div>
    }
</div>